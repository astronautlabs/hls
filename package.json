  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/chai": "^4.2.22",
    "@types/node": "^20.11.28",
    "chai": "^4.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
